\begin_layout Title
Older, but wiser: why older parents are more informative to offspring
\end_layout

\begin_layout Author
Bram Kuijper
\begin_inset Formula $^{1,2,3,4}$
\end_inset

 & Rufus A.
 Johnstone
\begin_inset Formula $^{5,6}$
\end_inset


\end_layout

\begin_layout Abstract
